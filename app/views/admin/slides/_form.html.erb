<%= form_for([:admin, @slide]) do |f| %>
  <%= f.text_field :title, placeholder: "Title" %><br>
  <%= f.text_field :message, placeholder: "Message" %><br>
  <%= f.file_field :image %>
  <%= select_tag("slide[expiration_date]", options_for_select(@dates, @dates.first), prompt: "Expiration Date") %>
  <%= f.submit %>
<% end %>
