<div class="app-body">
  <h1>All Slides</h1>
  <% @slides.each do |slide| %>

    <p><%= link_to slide.title_with_blanks, slide_path(slide) %> <%= button_to "Edit Slide", edit_slide_path(slide), method: :get %> <%= button_to "Delete Slide", slide_path(slide), method: :delete %></p>

  <% end %>
</div>
